{
  "name": "sick-fits-backend",
  "version": "2.0.0",
  "private": true,
  "author": "Wes Bos",
  "license": "MIT",
  "scripts": {
    "dev": "keystone",
    "seed-data": "keystone --seed-data"
  },
  "eslintConfig": {
    "extends": "wesbos/typescript.js",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0
    }
